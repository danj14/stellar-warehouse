    "lnd_d_substance": {
      "block_value": ".//*/..[@value='GcRealitySubstanceData.xml']",
      "table_schema": [
        {
          "field_name": "ID",
          "cursor": ".*/[@name='ID']",
          "key": "value"
        }
      ]
    },
    ":GcRealitySubstanceData.xml": {
      "table_alias": "lnd_d_substance",
      "name_index": [
        "Name",
        "NameLower",
        "ID",
        "BaseValue",
        "Category:GcRealitySubstanceCategory.xml=>SubstanceCategory",
        "Rarity:GcRarity.xml=>Rarity",
        "Legality:GcLegality.xml=>Legality",
        "ChargeValue",
        "StackMultiplier",
        "Cost:GcItemPriceModifiers.xml=>SpaceStationMarkup,LowPriceMod, HighPriceMod, BuyBaseMarkup, BuyMarkupMod",
        "NormalisedValueOnWorld",
        "NormalisedValueOffWorld",
        "tradeCategory:GcTradeCategory.xml=>TradingClass",
        "EconomyInfluenceMultiplier",
        "CookingIngredient",
        "GoodForSelling"
      ]
    },
    ":GcRefinerRecipe.xml": {
      "table_alias": "lnd_d_recipe",
      "name_index": [
        "Id",
        "Name",
        "TimeToMake",
        "Cooking",
        "Result:GcRefinerRecipeElement.xml=>Id!!ResultId,Type:GcInventoryType.xml=>InventoryType!!ResultInventoryType,Amount!!ResultAmount",
        "Ingredients:=>:GcRefinerRecipeElement.xml=>Id!!RecipeIngredientId,Type:GcInventoryType.xml=>InventoryType!!RecipeIngredientIdInventoryType,Amount!!RecipeIngredientAmount"
      ]
    },
    ":TkLocalisationEntry.xml": {
      "table_alias": "lnd_d_language",
      "name_index": [
        "Id",
        "English:VariableSizeString.xml=>Value"
      ]
    }
  },
  "TradingClassesData:": {
    "~name~:GcTradingClassData.xml": {
      "table_alias": "lnd_d_trade_class",
      "name_index": [
        "~name~!!ClassName",
        "Sells:GcTradeCategory.xml=>TradingClass!!SellsClassName",
        "Needs:GcTradeCategory.xml=>TradingClass!!NeedsClassName",
        "MinSellingPriceMultiplier",
        "MaxSellingPriceMultiplier",
        "MinBuyingPriceMultiplier",
        "MaxBuyingPriceMultiplier"
      ]
    }
  },
  "CategoryData:": {
    "~name~GcTradingCategoryData.xml":{
      "table_alias": "lnd_d_trade_category",
      "name_index": [
        "Name!!CategoryName",
        "ProductMultiplierChangePer100",
        "SubstanceMultiplierChangePer100"
      ]
    }
  },
  "Components:": {
    ":GcScannableComponentData.xml": {
      "table_alias": "lnd_d_farm_plant",
      "name_index": [
        "ScanRange",
        "ScanName",
        "DisableIfBuildingPart",
        "DisableIfInBase",
        "IconType:GcScannerIconTypes.xml=>ScanIconType",
        "ScannableType"
      ]
    },
    ":GcSimpleInteractionComponentData.xml": {
      "table_alias": "lnd_d_farm_plant",
      "name_index": [
        "Id",
        "Rarity:GcRarity.xml=>Rarity",
        "ActivationCost:GcInteractionActivationCost.xml=>SubstanceId,Cost!!ActivationCost,RequiredTech",
        "Name",
        "ScanType",
        "BaseBuildingTriggerActions:=>:GcInteractionBaseBuildingState.xml=>TriggerAction,Time",
        "CanCollectInMech"
      ]
    }
  }
}